<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="StrataHealth regression test suite" parallel="tests" thread-count="2" verbose="3" >
    <listeners>
        <listener class-name="com.stratahealth.testlisteners.ExtentReportListener"></listener>
        <listener class-name="com.stratahealth.testlisteners.AllureReportListener"></listener>

    </listeners>
    <test name="home page test" >

        <classes>
            <class name="com.stratahealth.tests.HomePageTest" />
        </classes>
    </test>

    <test name="contact page test" >
        <classes>
            <class name="com.stratahealth.tests.ContactPageTest" />
        </classes>
    </test>

    <test name="our customers page test" >
        <classes>
            <class name="com.stratahealth.tests.OurCustomersPageTest" />
        </classes>
    </test>

    <test name="about company page test" >
        <classes>
            <class name="com.stratahealth.tests.AboutCompanyPageTest" />
        </classes>
    </test>
    <test name="about news page test" >
        <classes>
            <class name="com.stratahealth.tests.AboutNewsPageTest" />
        </classes>
    </test>
    <test name="patient choice page test" >
        <classes>
            <class name="com.stratahealth.tests.OurImpactPatientChoicePageTest" />
        </classes>
    </test>


    <test name="siloed systems page test" >
        <classes>
            <class name="com.stratahealth.tests.OurImpactSiloedSystemsPageTest" />
        </classes>
    </test>



</suite>
